{
  "canisters": {
    "crypto-exchange-app": {
      "frontend": {
        "entrypoint": "build/index.html"
      },
      "source": [
        "build/"
      ],
      "type": "assets"
    },
    "ledger": {
      "type": "custom",
      "candid": "ledger.did",
      "wasm": "ledger.wasm",
      "init": "init.args"
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    },
    "canister_http": {
      "enabled": false
    },
    "proguard": {
      "enabled": false
    }
  },
  "dfx": "0.15.0",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
} 