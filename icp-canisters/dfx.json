{
  "canisters": {
    "hashed-time-lock": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package hashed-time-lock --release"
      ],
      "candid": "src/hashed_time_lock.did",
      "wasm": "target/wasm32-unknown-unknown/release/hashed_time_lock.wasm"
    },
    "ledger": {
      "type": "custom",
      "candid": "ledger.did",
      "wasm": "ledger.wasm",
      "init": "init.args"
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "dfx": "0.28.0",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "dev": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "ic": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    }
  },
  "version": 1
} 
